    "class-validator": ">=0.12.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.1",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": ">=5.59.9",
    "@typescript-eslint/parser": ">=5.59.9",
    "eslint": ">=8.43.0",
    "eslint-config-prettier": ">=8.8.0",
    "prettier": ">=2.8.8",
    "typescript": "~5.0.0"
