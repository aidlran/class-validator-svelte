{
  "name": "class-validator-svelte",
  "version": "1.0.0-PREVIEW",
  "private": true,
  "description": "Simple yet powerful form validation for SvelteKit by integrating class-validator.",
  "author": "Aidan Loughran (https://loughran.dev)",
  "license": "MIT",
  "repository": "github:aidlran/class-validator-svelte",
  "homepage": "https://github.com/aidlran/class-validator-svelte#readme",
  "bugs": "https://github.com/aidlran/class-validator-svelte/issues",
  "keywords": [
    "sveltekit",
    "svelte",
    "form-validation",
    "validation-framework",
    "validation",
    "class-validator",
